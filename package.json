{
  "name": "desafio-softeo-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "seed": "npx prisma generate && npx prisma migrate dev && ts-node prisma/seed.ts",
    "start": "nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts",
    "start-docker": "sudo service postgresql stop && docker-compose up -d && nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts",
    "dev": "npx ts-node src/server.ts",
    "lint": "eslint src/**/*.ts",
    "migrate": "npx prisma migrate dev --name init",
    "test": "mocha -r ts-node/register src/__tests__/unit/**/*.test.ts --exit -t 60000",
    "format": "eslint src/**/*.ts --fix"
  },
  "repository": {
