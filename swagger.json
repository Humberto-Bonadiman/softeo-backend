{
  "openapi": "3.0.1",
  "info": {
    "title": "Dentist",
    "description": "Documentation from dentist api",
    "version": "1.0.0"
  },
  "basePath": "/",
  "paths": {
    "/dentist": {
      "post": {
        "tags": ["Dentist"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "example": {
                  "email": "your_email_here@email.com",
                  "name": "Your Email Here",
                  "password": "your-password-here"
                }
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "401": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Create a new dentist"
          }
        }
      },
      "get": {
        "tags": ["Dentist"],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "200": {
            "description": "List all dentists"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": ["Dentist"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "example": {
                  "email": "your_email_here@email.com",
                  "password": "your-password-here"
                }
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "401": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Login with a existent dentist"
          }
        }
      }
    }
  }
}